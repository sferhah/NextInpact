<Application
    x:Class="NextInpact.Native.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:NextInpact.Native.UWP"
    xmlns:converters="using:NextInpact.Native.UWP.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <converters:ByteArrayToImageConverter x:Key="ByteArrayToImageConverter" />
            <converters:DoubleToPercentageConverter x:Key="DoubleToPercentageConverter" />
            <converters:RawHtmlToStringConverter x:Key="RawHtmlToStringConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
